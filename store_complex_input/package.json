{
  "name": "ava-testing",
  "version": "1.0.0",
  "license": "(MIT AND Apache-2.0)",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "test-rs": "$npm_execpath run build-rs && ava -- ./contract-rs/target/wasm32-unknown-unknown/release/store_complex_input.wasm",
    "test-ts": "$npm_execpath run build-ts && ava -- ./contract-ts/build/store_complex_input.wasm",
