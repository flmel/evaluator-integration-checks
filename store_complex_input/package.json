    "build-ts": "cd contract-ts && $npm_execpath run build",
    "postinstall": "cd contract-ts && $npm_execpath install"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.4",
    "@types/node": "^20.8.10",
    "ava": "^5.3.1",
    "borsh": "^2.0.0",
    "near-workspaces": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
